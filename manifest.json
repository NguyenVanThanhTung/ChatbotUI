{
  "manifest_version": 3,
  "name": "Cosafe Chat",
  "version": "1.0",
  "description": "Chat with Cosafe in a popup window",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/icons/cosafeIcon.png",
      "48": "assets/icons/cosafeIcon.png",
      "128": "assets/icons/cosafeIcon.png"
    }
  },
  "icons": {
    "16": "assets/icons/cosafeIcon.png",
    "48": "assets/icons/cosafeIcon.png",
    "128": "assets/icons/cosafeIcon.png"
  },
  "permissions": ["storage"]
}